
 <div class="form-container">
  <%= form_for :session, url: login_path do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>
    <br>
    <br>
      <%= f.label :password %>
      <%= f.password_field :password%>
    <br>
    <%= f.submit class: "form-btn", value: "Submit" %>
  <% end %>
  
 
</div>